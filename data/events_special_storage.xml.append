<mod:findName type="event" name="STORAGE_CHECK_BUTTON">
	<mod:findWithChildLike type="choice" child-type="text" panic="true">
        <mod:selector>Modify a modular weapon.</mod:selector>
        <mod:removeTag />
    </mod:findWithChildLike>
	<mod-append:choice req="HEKTAR_MODULAR_WEAPONS_STORAGE" hidden="true" blue="false">
        <text>Modify a modular weapon.</text>
        <event load="STORAGE_CHECK_MODULAR"/>
    </mod-append:choice>

</mod:findName>

<mod:findName type="event" name="STORAGE_CHECK_MODULAR">
    <mod:findWithChildLike type="choice" child-type="text" panic="true">
        <mod:selector>Nevermind.</mod:selector>
        <mod:removeTag />
    </mod:findWithChildLike>
    <mod-append:choice hidden="true">
        <text>Nevermind.</text>
        <event load="STORAGE_CHECK"/>
    </mod-append:choice>

	<mod-append:choice req="ARTILLERY_MODULAR_LASER" blue="false" hidden="true">
		<text>Configure the Modular Artillery.</text>
		<event load="STORAGE_CHECK_MODULAR_ARTILLERY"/>
	</mod-append:choice>
    <mod-append:choice req="ARTILLERY_MODULAR_MISSILE" blue="false" hidden="true">
		<text>Configure the Modular Artillery.</text>
		<event load="STORAGE_CHECK_MODULAR_ARTILLERY"/>
	</mod-append:choice>
    <mod-append:choice req="ARTILLERY_MODULAR_BEAM" blue="false" hidden="true">
		<text>Configure the Modular Artillery.</text>
		<event load="STORAGE_CHECK_MODULAR_ARTILLERY"/>
	</mod-append:choice>

    <mod-append:choice req="HEKTAR_MODULAR_FOCUS" blue="false" hidden="true">
		<text>Configure a Modular Pinpoint.</text>
		<event load="STORAGE_CHECK_MODULAR_FOCUS"/>
	</mod-append:choice>
	<mod-append:choice req="HEKTAR_MODULAR_SHOTGUN" blue="false" hidden="true">
		<text>Configure a Modular Flak.</text>
		<event load="STORAGE_CHECK_MODULAR_SHOTGUN"/>
	</mod-append:choice>
	<mod-append:choice req="HEKTAR_MODULAR_BOMB" blue="false" hidden="true">
		<text>Configure a Modular Bomb Launcher.</text>
		<event load="STORAGE_CHECK_MODULAR_BOMB"/>
	</mod-append:choice>

    <mod-append:choice req="HEKTAR_MODULAR_DRONES_LASERS" blue="false" hidden="true">																																							:choice req="HEKTAR_MODULAR_DRONE_LASERS" blue="false" hidden="true">
		<text>Configure a Modular Laser Drone.</text>
		<event load="STORAGE_CHECK_MODULAR_DRONE_LASER"/>
	</mod-append:choice>
	<mod-append:choice req="HEKTAR_MODULAR_DRONES_BEAMS" blue="false" hidden="true">
		<text>Configure a Modular Beam Drone.</text>
		<event load="STORAGE_CHECK_MODULAR_DRONE_BEAM"/>
	</mod-append:choice>
	<mod-append:choice req="HEKTAR_MODULAR_DRONES_IONS" blue="false" hidden="true">
